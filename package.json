{
  "name": "tailorbird",
  "version": "0.0.1",
  "description": "A powerfull tool for doing inline styles and themes in React.",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run build",
    "start": "node server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregthebusker/tailorbird.git"
  },
  "author": "Greg Schechter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gregthebusker/tailorbird/issues"
  },
  "homepage": "https://github.com/gregthebusker/tailorbird#readme",
  "dependencies": {
    "babel": "^6.0.15",
    "babel-core": "^6.1.4",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.1.4",
    "babel-plugin-transform-react-inline-elements": "^6.1.4",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "babel-preset-stage-1": "^6.1.2",
    "hoist-non-react-statics": "^1.0.3",
    "radium": "^0.14.3",
    "webpack": "^1.12.4",
    "webpack-config-merger": "0.0.3"
  },
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "eslint": "^1.7.1",
    "eslint-plugin-react": "^3.5.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "jest-cli": "^0.5.10",
    "jest-webpack": "^0.2.1",
    "react": "^0.14.2",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.3.4",
    "webpack-bundle-tracker": "0.0.8",
    "webpack-dev-server": "^1.12.1"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "./node_modules/",
      "./src"
    ]
  },
}
